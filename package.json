{
  "name": "noserverconvert",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:web",
    "dev:web": "BUILD_TARGET=web vite",
    "dev:ext": "BUILD_TARGET=extensions vite",
    "build": "npm run build:web && npm run build:ext",
    "build:web": "BUILD_TARGET=web vite build",
    "build:ext": "BUILD_TARGET=extensions vite build",
    "preview": "npm run preview:web",
    "preview:web": "BUILD_TARGET=web vite preview",
    "preview:ext": "BUILD_TARGET=extensions vite preview",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@ffmpeg/core": "^0.12.10",
    "@ffmpeg/core-mt": "^0.12.10",
    "@ffmpeg/ffmpeg": "0.12.10",
    "@ffmpeg/util": "^0.12.2",
    "@types/uuid": "^10.0.0",
    "jspdf": "^3.0.1",
    "jszip": "^3.10.1",
    "libraw.js": "^3.0.0",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^5.3.31",
    "sharp": "^0.34.2",
    "uuid": "^11.1.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.0",
    "@types/node": "^24.0.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "@vueuse/core": "^13.3.0",
    "@vueuse/head": "^2.0.0",
    "jsdom": "^26.1.0",
    "pinia": "^3.0.3",
    "sass": "^1.89.2",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.3",
    "vue-router": "^4.5.1",
    "vue-tsc": "^2.2.8"
  }
}
